<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="eu.crewbase.lineup.tmp.thirdclass.ThirdClassEdit"
        datasource="thirdClassDs"
        focusComponent="fieldGroup"
        messagesPack="eu.crewbase.lineup.tmp.thirdclass">
    <dsContext>
        <datasource id="thirdClassDs"
                    class="eu.crewbase.lineup.entity.tmp.ThirdClass"
                    view="thirdClass-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="thirdClassDs">
            <column width="250px">
                <field property="thirdAttr"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
