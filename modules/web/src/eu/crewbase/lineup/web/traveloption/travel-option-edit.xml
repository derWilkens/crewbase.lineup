<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="eu.crewbase.lineup.web.traveloption.TravelOptionEdit"
        datasource="travelOptionDs"
        focusComponent="fieldGroup"
        messagesPack="eu.crewbase.lineup.web.traveloption">
    <dsContext>
        <datasource id="travelOptionDs"
                    class="eu.crewbase.lineup.entity.wayfare.TravelOption"
                    view="mailNotification-manage"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="travelOptionDs">
            <column width="250px">
                <field property="transfer"/>
                <field property="bookedSeats"/>
                <field property="favoriteTrip"/>
                <field property="status"/>
                <field property="availableSeate"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
