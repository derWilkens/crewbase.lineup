<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="eu.crewbase.lineup.web.modeoftransfer.ModeOfTransferEdit"
        datasource="modeOfTransferDs"
        focusComponent="fieldGroup"
        messagesPack="eu.crewbase.lineup.web.modeoftransfer">
    <dsContext>
        <datasource id="modeOfTransferDs"
                    class="eu.crewbase.lineup.entity.coredata.ModeOfTransfer"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="modeOfTransferDs">
            <column width="250px">
                <field property="mode"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
